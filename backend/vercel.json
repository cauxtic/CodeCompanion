{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
  "Access-Control-Allow-Origin": "https://code-companion-bdln.vercel.app, http://localhost:3000",
  "Access-Control-Allow-Credentials": "true"
}
      }
    ]
  }